{
    "General": {
        "Precision": "double",
        "Dim": "3",
        "IsTest": "false"
    },
    "SpatialResolutionFluid": {
        "Degree": "2",
        "RefineSpace": "0"
    },
    "SpatialResolutionStructure": {
        "Degree": "1",
        "RefineSpace": "0"
    },
